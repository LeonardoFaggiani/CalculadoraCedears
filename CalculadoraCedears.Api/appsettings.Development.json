{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GoogleClient": {
    "AuthTokenUri": "https://oauth2.googleapis.com/token",
    "Secret": "$GOOGLE_SECRET",
    "RedirectUri": "$GOOGLE_REDIRECT_URI",
    "Id": "$GOOGLE_CLIENT_ID"
  },
  "JwtOptions": {
    "Secret": "$JWT_SECRET", //Important, save the secret key in services likes Key Vault (Azure), KMS (AWS), Cloud KMS (GCP), etc.
    "Issuer": "$JWT_ISSUER",
    "Audience": "$JWT_AUDIENCE",
    "ExpiryMinutes": 5
  },
  "SQL": {
    "ConnectionStrings": "$SQL_CONNECTION_STRING"
  }
}